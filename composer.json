{
    "name": "mibo/taxonomy",
    "type": "library",
    "description": "PHP lib for taxonomy",
    "keywords": [
        "PHP",
        "Prices",
        "Price",
        "VAT",
        "Tax",
        "Taxes",
        "Classification",
        "Product Classification",
        "Product Taxonomy",
        "MiBo",
        "Library",
        "Lib",
        "Package",
        "Money"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiBo\\Taxonomy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Taxonomy\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpunit/php-invoker": "^5.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "slevomat/coding-standard": "^8.13",
        "phpstan/extension-installer": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0"
    },
    "config": {
        "platform-check": true,
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "phpstan/extension-installer": true
        }
    }
}
