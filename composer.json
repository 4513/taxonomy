{
    "name": "mibo/taxonomy",
    "type": "library",
    "description": "PHP lib for taxonomy",
    "keywords": [
        "PHP",
        "Prices",
        "Price",
        "VAT",
        "Tax",
        "Taxes",
        "Classification",
        "Product Classification",
        "Product Taxonomy"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiBo\\Taxonomy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Taxonomy\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpunit/php-invoker": "^4.0",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "slevomat/coding-standard": "^8.13",
        "phpstan/extension-installer": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3"
    },
    "config": {
        "platform-check": true,
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MiBo\\Properties\\Providers\\ConfigProvider",
                "MiBo\\Properties\\Providers\\FacadeProvider",
                "MiBo\\Properties\\Providers\\ServiceProvider",
                "MiBo\\Properties\\Providers\\TranslationProvider"
            ]
        }
    }
}
